{
	"extends": [
		"../../.eslintrc.json",
		"neon/common",
		"neon/browser",
		"neon/node",
		"neon/typescript",
		"neon/angular",
		"neon/edge",
		"neon/prettier"
	],
	"parserOptions": {
		"project": "./tsconfig.eslint.json"
	},
	"ignorePatterns": [
		"**/dist/*"
	],
	"rules": {
		"@typescript-eslint/consistent-type-definitions": [
			"error",
			"interface"
		],
		"no-html-link-for-pages": 0
	}
}